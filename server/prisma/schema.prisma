generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  mobile    String
  postcode  String
  services  String // ["delivery", "pick-up", "payment"]
  createdAt DateTime @default(now())
}
